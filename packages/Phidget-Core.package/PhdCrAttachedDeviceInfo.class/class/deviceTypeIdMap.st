		at: 16r153 	put: #TextLCD2x20w088;					"PHIDID_TEXTLCD_2x20_w_0_8_8 = 0x153"
		at: 16r17D 	put: #TextLCD2x20w888;					"PHIDID_TEXTLCD_2x20_w_8_8_8 = 0x17D"
		at: 16r049 	put: #TextLED1x8;						"PHIDID_TEXTLED_1x8 = 0x049"
		at: 16r048 	put: #TextLED4x8;						"PHIDID_TEXTLED_4x8 = 0x048"
		at: 16r07A 	put: #UnipolarStepper4Motor;			"PHIDID_UNIPOLAR_STEPPER_4MOTOR = 0x07A"
		at: 16r072 	put: #WeightSensor.						"PHIDID_WEIGHTSENSOR = 0x072"

	^ DeviceTypeIdMap