type and class ID maps
deviceTypeIdMap
	"Answer a map from phidget device type id in phidget21 to symbols representing that phidget type.
	See the phidget21 documentation at CPhidget_DeviceID to see where these values come from."

	"DeviceTypeIdMap ifNotNil: [ ^ DeviceTypeIdMap ]."
	
	DeviceTypeIdMap := Dictionary new: 43.
	DeviceTypeIdMap
		at: 16r071 	put: #Accelerometer2Axis;				"PHIDID_ACCELEROMETER_2AXIS = 0x071"
		at: 16r07E 	put: #Accelerometer3Axis; 				"PHIDID_ACCELEROMETER_3AXIS = 0x07E"
		at: 16r082 	put: #AdvancedServo1Motor;				"PHIDID_ADVANCEDSERVO_1MOTOR = 0x082"
		at: 16r03A 	put: #AdvancedServo8Motor;				"PHIDID_ADVANCEDSERVO_8MOTOR = 0x03A"
		at: 16r07B 	put: #BipolarStepper1Motor;				"PHIDID_BIPOLAR_STEPPER_1MOTOR = 0x07B"
		at: 16r04B 	put: #Encoder1Encoder1Input;			"PHIDID_ENCODER_1ENCODER_1INPUT = 0x04B"
		at: 16r080 	put: #EncoderHS1Encoder;				"PHIDID_ENCODER_HS_1ENCODER = 0x080"
		at: 16r04F 	put: #EncoderHS4Encoder4Input; 			"PHIDID_ENCODER_HS_4ENCODER_4INPUT = 0x04F"
		at: 16r099 	put: #Generic;							"PHIDID_GENERIC = 0x099"
		at: 16r079 	put: #GPS;								"PHIDID_GPS = 0x079"
		at: 16r075 	put: #Gyroscope;						"PHIDID_GYROSCOPE = 0x075"
		at: 16r040 	put: #InterfaceKit004;					"PHIDID_INTERFACEKIT_0_0_4 = 0x040"
		at: 16r081 	put: #InterfaceKit008;					"PHIDID_INTERFACEKIT_0_0_8 = 0x081"
		at: 16r044 	put: #InterfaceKit01616;					"PHIDID_INTERFACEKIT_0_16_16 = 0x044"
		at: 16r051 	put: #InterfaceKit057;					"PHIDID_INTERFACEKIT_0_5_7 = 0x051"
		at: 16r053 	put: #InterfaceKit088wLCD;				"PHIDID_INTERFACEKIT_0_8_8_w_LCD = 0x053"
		at: 4 		put: #InterfaceKit488;					"PHIDID_INTERFACEKIT_4_8_8 = 4"
		at: 16r045 	put: #InterfaceKit888;					"PHIDID_INTERFACEKIT_8_8_8 = 0x045"
		at: 16r07D 	put: #InterfaceKit888wLCD;				"PHIDID_INTERFACEKIT_8_8_8_w_LCD = 0x07D"
		at: 16r04D 	put: #IR;								"PHIDID_IR = 0x04D"
		at: 16r04A 	put: #LED64;							"PHIDID_LED_64 = 0x04A"
		at: 16r04C 	put: #LED64Adv;							"PHIDID_LED_64_ADV = 0x04C"
		at: 16r076 	put: #LinearTouch;						"PHIDID_LINEAR_TOUCH = 0x076"
		at: 16r059 	put: #MotorControlHC2Motor;				"PHIDID_MOTORCONTROL_HC_2MOTOR = 0x059"
		at: 16r058 	put: #MotorControlLV2Motor4Input;		"PHIDID_MOTORCONTROL_LV_2MOTOR_4INPUT = 0x058"
		at: 1 		put: #Nothing;							"PHIDID_NOTHING = 1"
		at: 16r074 	put: #PHSensor;							"PHIDID_PHSENSOR = 0x074"
		at: 16r030 	put: #RFID;								"PHIDID_RFID = 0x030"
		at: 16r031 	put: #RFID2Output;						"PHIDID_RFID_2OUTPUT = 0x031"
		at: 16r077 	put: #RotaryTouch;						"PHIDID_ROTARY_TOUCH = 0x077"
		at: 16r039 	put: #Servo1Motor;						"PHIDID_SERVO_1MOTOR = 0x039"
		at: 2 	 	put: #Servo1MotorOld;					"PHIDID_SERVO_1MOTOR_OLD = 2"
		at: 16r038 	put: #Servo4Motor;						"PHIDID_SERVO_4MOTOR = 0x038"
		at: 3 		put: #Servo4MotorOld;					"PHIDID_SERVO_4MOTOR_OLD = 3"
		at: 16r07F 	put: #SpacialAccel3Axis; 				"PHIDID_SPACIAL_ACCEL_3AXIS = 0x07F"
		at: 16r04E 	put: #SpacialAccel3Gyro1; 				"PHIDID_SPACIAL_ACCEL_3_GYRO_1 = 0x04E"
		at: 16r070 	put: #TemperatureSensor;				"PHIDID_TEMPERATURESENSOR = 0x070"
		at: 16r32 	put: #TemperatureSensor4;				"PHIDID_TEMPERATURESENSOR_4 = 0x032"
		at: 16r052 	put: #TextLCD2x20;						"PHIDID_TEXTLCD_2x20 = 0x052"
